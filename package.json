{
  "name": "vue2-shortcut",
  "version": "0.1.5",
  "description": "A Vue2.x plugin to quickly create shortcuts.",
  "main": "./dist/shortcut.js",
  "scripts": {
    "test-unit": "jest",
    "format": "prettier --write \"dist/**/*.js\" \"src/**/*.ts\"",
    "lint": "eslint --fix \"dist/**/*.js\"",
    "build": "npm run test-unit && tsc && npm run format && npm run lint",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Graxi/v-shortcut"
  },
  "keywords": [
    "vue",
    "shortcut"
  ],
  "author": "Graxi <graxixiao@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/**/*"
  ]
}
